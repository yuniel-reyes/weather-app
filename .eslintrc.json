{
    "extends": [
        "plugin:prettier/recommended",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "env":{
        "es6": true
    },
    "plugins": ["prettier"],
    "rules": {"prettier/prettier": [
        "error", {"endOfLine": "auto"}
    ]}
}